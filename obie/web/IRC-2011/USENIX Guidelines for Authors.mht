var hasImage=3Dfalse;=0A=
// Check on Browser type=0A=
bName =3D navigator.appName;=0A=
bVer =3D parseInt(navigator.appVersion);=0A=
if ((bName =3D=3D "Netscape" && bVer >=3D 3) ||=0A=
    (bName =3D=3D "Mozilla" && bVer >=3D 5) ||=0A=
    (bName =3D=3D "Microsoft Internet Explorer" && bVer >=3D 4)) {=0A=
    hasImage =3D true;=0A=
    // Create an associative array of buttons=0A=
    Buttons =3D new Array();=0A=
    // create buttons=0A=
    crButton('home');=0A=
    crButton('about');=0A=
    crButton('events');=0A=
    crButton('membership');=0A=
    crButton('publications');=0A=
    crButton('students');	=0A=
}=0A=
// Create a button object=0A=
function Button(file) {=0A=
    this.main =3D new Image();=0A=
    this.main.src =3D "/graphics/small" + file + ".gif";=0A=
    this.over =3D new Image();=0A=
    this.over.src =3D "/graphics/over/small" + file + ".gif";=0A=
}=0A=
function crButton(name) {=0A=
    Buttons[name] =3D new Button(name);=0A=
}=0A=
function iover(name)=0A=
{=0A=
    if (hasImage =3D=3D true) {=0A=
	document[name].src =3D Buttons[name].over.src;=0A=
    }=0A=
}=0A=
function iout(name)=0A=
{=0A=
    if (hasImage =3D=3D true) {=0A=
	document[name].src =3D Buttons[name].main.src;=0A=
    }=0A=
}=0A=

